<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MaxWidthFrameLayout">
        <attr name="maxWidthLayout" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomFontTextView">
        <attr name="customFont" format="string" />
    </declare-styleable>

    <declare-styleable name="CircularImageView">
        <attr name="border" format="boolean" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="shadow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Theme">
        <attr name="circularImageViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="IconButton">
        <attr name="labelText" format="string" localization="suggested" />
        <attr name="labelColor" format="color" />
        <attr name="iconText" format="string" />
        <attr name="iconColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ScrimInsets">
        <attr name="insetFg" format="color|reference"/>
    </declare-styleable>

</resources>

